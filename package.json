{
  "name": "orange",
  "version": "1.0.0",
  "description": "Electron and TypeScript UI client for Bitcoin Core",
  "license": "Unlicense",
  "scripts": {
    "wds": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "build-renderer": "webpack --config webpack.renderer.config.js",
    "electron": "npm run build-main && electron ./dist/main/main.js",
    "build-main": "webpack --config webpack.main.config.js && npm run copy-bitcoind",
    "copy-bitcoind": "cp -R src/vendor dist/vendor",
    "lint": "eslint '**/*.{ts,tsx,js}'",
    "lint-fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@hot-loader/react-dom": "^16.11.0",
    "@material-ui/core": "^4.8.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.37",
    "@types/bluebird": "^3.5.29",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/fs-extra": "^8.0.1",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/styled-components": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-loader": "^8.0.6",
    "bluebird": "^3.7.2",
    "clean-webpack-plugin": "^3.0.0",
    "clsx": "^1.0.4",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.0",
    "electron": "^6.0.8",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "image-webpack-loader": "^6.0.0",
    "lint-staged": "^9.5.0",
    "moment": "^2.24.0",
    "npm-run-all": "^4.1.5",
    "pkg-dir": "^4.2.0",
    "prettier": "1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.2",
    "styled-components": "^4.4.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
