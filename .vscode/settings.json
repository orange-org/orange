{
  "search.exclude": {
    "artifacts": true,
    "coverage": true
  },
  "cSpell.words": [
    "Bcore",
    "Erroring",
    "Executables",
    "Mempool",
    "SCROLLABLE",
    "Satoshi",
    "bitcoind",
    "clsx",
    "formik",
    "testid"
  ],
  "editor.formatOnSave": true,
  "editor.rulers": [80],
  "rewrap.wrappingColumn": 80,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}
